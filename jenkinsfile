pipeline {
    agent any

    stages {
        stage('git colne') {
            steps {
                echo 'git clone main';
                    git branch: 'main',
                    url: 'https://github.com/chJasser/teacherDevops.git',
                    credentialsId: 'c9c32571-8aff-42d6-b416-215d5c01f15f';

            }

        }
           stage('clean') {
            steps {
            
                sh 'mvn clean'
            }
        }
           stage('build') {
            steps {
            
                sh 'mvn build'
            }
        }
        
    
           stage('test') {
            steps {
            
            sh 'mvn test'

            }

        }
            stage('install') {
            steps {
            
            sh 'mvn install'

            }

        }
              stage('run as spring boot') {
            steps {
            
            sh 'mvn spring-boot:run'

            }

        }
    }
}